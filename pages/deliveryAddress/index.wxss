.container {
  width: 100%;
  height: 100%;
  background-color: #f9f9f9;
  position: fixed;
  top: 0;
  left: 0;
}

.list {
  width: 100%;
  height: 40px;
  line-height: 40px;
  background-color: #fff;
  border-bottom: 1px #dedede solid;
  overflow: hidden;
}

.address {
  width: 100%;
  background-color: #fff;
  border-bottom: 1px #dedede solid;
  overflow: hidden;
}

.city {
  width: 100%;
  padding: 0 10px;
}

input {
  padding: 0 10px;
  height: 40px;
  line-height: 40px;
}

textarea {
  height: 60px;
  padding: 10px;
  position: unset;
}

button[type="primary"] {
  color: #fff;
  background-color: #e64340;
  margin: 10px;
}
