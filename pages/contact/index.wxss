.container {
  width: 100%;
  height: 100%;
  background-color: #f9f9f9;
  position: fixed;
  top: 0;
  left: 0;
}

.list-con {
  width: 100%;
  background-color: #fff;
  margin-top: 10px;
  padding: 0 0 0 10px;
  border-top: 1px #dedede solid;
  border-bottom: 1px #dedede solid;
}

.list {
  height: 36px;
  line-height: 36px;
  padding: 0 10px 0 0;
  background-color: #fff;
  border-bottom: 1px #dedede solid;
  overflow: hidden;
}

.list .name {
  float: left;
}

.list .info {
  width: 50%;
  float: right;
  padding-right: 5px;
  text-align: right;
  color: #999;
}

.list .iconfont {
  float: right;
  color: #ccc;
  padding-right: 10px;
}

.list:last-child {
  border: none;
}
