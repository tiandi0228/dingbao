.container {
  width: 100%;
  height: 100%;
  background-color: #f9f9f9;
  position: fixed;
  top: 0;
  left: 0;
}

.box {
  margin: 20px 10px;
  background-color: #fff;
  padding: 10px;
  border: 1px #dedede solid;
  border-radius: 5px;
}

.box .item {
  display: block;
  line-height: 30px;
  font-size: 14px;
}

button[type="primary"] {
  color: #fff;
  background-color: #1aad19;
  margin: 0 10px;
}
