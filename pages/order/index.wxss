.container {
  width: 100%;
  height: 100%;
  background-color: #f9f9f9;
  position: fixed;
  top: 0;
  left: 0;
}

.list-con {
  width: 100%;
  background-color: #fff;
  border-top: 1px #dedede solid;
  border-bottom: 1px #dedede solid;
}

.list {
  padding: 10px;
  background-color: #fff;
  border-bottom: 1px #dedede solid;
  overflow: hidden;
}

.list .title {
  float: left;
  width: 80%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.list .iconfont {
  float: right;
  color: #ccc;
}

.list:last-child {
  border: none;
}
